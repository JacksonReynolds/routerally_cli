
Gem::Specification.new do |s|
  s.name        = 'Route Rally'
  s.version     = '1.0.0'
  s.date        = '2020-07-29'
  s.summary     = "Route Rally uses input from the user to use the getRoutesForLatLon method of the Mountain Project API to find data on routes close to a location provided by the user. There are also a slew of other optional arguments that the user provides."
  s.description = "A gem that provides info on climbing routes in a given area"
  s.authors     = ["Jackson Reynolds"]
  s.email       = 'jwreynold@gmail.com'
  s.files       = ["lib/"]
  s.homepage    =
    'https://rubygems.org/gems/hola'
  s.license       = 'MIT'
end